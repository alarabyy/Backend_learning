@model IEnumerable<Port.ViewModel.RegisterUserViewModel>

@{
    ViewData["Title"] = "All Profiles";
}

<h1>All Profiles</h1>

<table class="table table-striped  table-hover">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.Phone</td>
            </tr>
        }
    </tbody>
</table>
